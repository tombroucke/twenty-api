{
  "name": "tombroucke/twenty-api",
  "description": "REST API connector for twenty CRM",
  "type": "library",
  "require": {
    "fansipan/fansipan": "1",
    "guzzlehttp/guzzle": "^7.9"
  },
  "autoload": {
    "psr-4": {
      "Otomaties\\Twenty\\": "app/"
    }
  },
  "authors": [
    {
      "name": "Tom Broucke",
      "email": "tom@tombroucke.be"
    }
  ],
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "require-dev": {
    "laravel/pint": "^1.22"
  }
}
